{
  "manifestVersion": 1,
  "id": "uft-build-tasks",
  "name": "HPE Application Automation Tools Extension",
  "version": "1.0.0",
  "publisher": "fabrikam",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Tools for test with UFT. Includes 4 build tasks.",
  "categories": [
    "Test"
  ],
  "resources": [
    "some stuff"
  ],
  "tags": [
    "UFT",
    "HPE ALM"
  ],
  "icons": {
    "default": "resources/img/tfs.png"
  },
  "screenshots ": [
    {
      "path": "resources/screenshots/AlmExecution.PNG"
    },
    {
      "path": "resources/screenshots/screen2.png"
    }
  ],
  "links": {
    "home": {
      "uri": "https://github.com/hpsa/ADM-TFS-Extension"
    },
    "installation": {
      "uri": "https://github.com/hpsa/ADM-TFS-Extension/wiki/Installing-the-ADM-TFS-Extension"
    },
    "learn": {
      "uri": "https://github.com/hpsa/ADM-TFS-Extension/wiki/HPE-ADM-TFS-Extension"
    },
  },
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "branding": {
    "color": "rgb(89, 141, 88)",
    "theme": "dark"
  },
  "files": [
    {
      "path": "AlmLabEnvPrepareTask"
    },
    {
      "path": "AlmLabManagementTask"
    },
    {
      "path": "RunFromALM"
    },
    {
      "path": "RunFromFileSystemTask"
    }
  ],
  "scopes": [
    "vso.test",
    "vso.build_execute",
    "vso.serviceendpoint"
  ],
  "contributions": [
    {
      "id": "custom-build-task-1",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "AlmLabEnvPrepareTask"
      }
    },
    {
      "id": "custom-build-task-2",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "AlmLabManagementTask"
      }
    },
    {
      "id": "custom-build-task-3",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "RunFromALM"
      }
    },
    {
      "id": "custom-build-task-4",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "RunFromFileSystemTask"
      }
    }
  ]
}
